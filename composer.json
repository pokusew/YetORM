
{
	"name": "pokusew/yetorm",
	"type": "library",
	"description": "Lightweight ORM for Nette\\Database",
	"keywords": ["nette", "orm", "database"],
	"homepage": "https://github.com/pokusew/YetORM",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/pokusew/YetORM/issues"
	},

	"require": {
		"php": ">=5.3.0",
		"nette/utils": "2.*",
		"nette/reflection": "2.*",
		"nette/database": "2.*",
		"uestla/aliaser": "1.*"
	},

	"require-dev": {
		"nette/tester": "*",
		"nette/caching": "*",
		"nette/robot-loader": "*"
	},

	"autoload": {
		"psr-0": {
			"YetORM": "src/"
		}
	}
}
